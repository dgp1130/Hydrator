/** @fileoverview Config for the main library compilation. */

{
  "extends": "./tsconfig.base.json",
  "include": ["src/**/*.ts"],
  "compilerOptions": {
    "composite": false,
    "outDir": "dist/src/",
  },
  "exclude": [
    "src/**/*.test.ts",
    "src/testing/**/*.ts",
    "src/demo/**.ts",
  ],
}
